	<skill id="8870" levels="1" name="Tauti's Ring">
    <!-- Increases Critical Damage by 15%,
		Skill Attack by 5%, 
		damage inflicted during PvP by 5%, 
		resistance to Paralysis/Yoke/Exile by 10, 
		resistance to Damage Reflect by 5, resistance to HP Drain by 5, and decreases damage received during PvP by 5%. Increases Critical by 54 when equipped with a blunt. -->
        <set name="magicLevel" val="99"/>
        <set name="target" val="TARGET_SELF"/>
        <set name="skillType" val="BUFF"/>
        <set name="operateType" val="OP_PASSIVE"/>
        <for>
			<add order="0x40" stat="defenceUnholy" val="25"/>
			<add order="0x40" stat="defenceFire" val="25"/>
            <mul order="0x30" stat="cAtk" val="1.15"/>
            <mul order="0x30" stat="skillPower" val="1.05"/>
            <mul order="0x30" stat="pvpPhysDmgBonus" val="1.05"/>
            <add order="0x40" stat="rootResist" val="10"/>
            <add order="0x40" stat="paralyzeResist" val="10"/>
            <mul order="0x30" stat="pvpPhysDefenceBonus" val="1.05"/>
            <mul order="0x30" stat="pvpPhysSkillDefenceBonus" val="1.05"/>
            <mul order="0x30" stat="pvpMagicSkillDefenceBonus" val="1.05"/>
            <add order="0x40" stat="rCrit" val="54">
                <using kind="Sword, Big Sword"/>
            </add>
        </for>
    </skill>

		<set name="icon" val="BranchSys2.icon.skill20006"/>
		<table name="#effectPoint">611</table>
		<table name="#magicLevel">62</table>
		<set name="reuseDelay" val="240000"/>
		<set name="hitTime" val="1500"/>
		<set name="hitCancelTime" val="350"/>
		<set name="mpConsume1" val="58"/>
		<set name="mpConsume2" val="14"/>
		<set name="effectPoint" val="#effectPoint"/>
		<set name="magicLevel" val="#magicLevel"/>
		<set name="target" val="TARGET_SELF"/>
		<set name="skillType" val="BUFF"/>
		<set name="operateType" val="OP_ACTIVE"/>
		<for>
			<effect count="1" name="Buff" time="600" val="0">
				<mul order="0x30" stat="maxHp" val="1.15"/>
				<mul order="0x30" stat="maxCp" val="1.15"/>
			</effect>
			<effect count="1" name="HealPercent" time="0" val="15"/>
			<effect count="1" name="HealCPPercent" time="0" val="15"/>
		</for>

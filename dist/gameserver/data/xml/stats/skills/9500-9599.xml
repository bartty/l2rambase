    <skill id="9544" levels="1" name="Power Herb">
    <!-- Increases P. Atk. By 100% and M. Atk. By 400% for 30 seconds. -->
        <set name="icon" val="icon.etc_warrior_set_i00"/>
        <set name="operateType" val="OP_ACTIVE"/>
        <set name="target" val="TARGET_SELF"/>
        <set name="skillType" val="BUFF"/>
        <set name="reuseDelay" val="2000"/>
        <set name="magicType" val="SPECIAL"/>
        <set name="hitTime" val="100"/>
        <set name="isHideUseMessage" val="true"/>
        <set name="isHandler" val="true"/>
        <set name="altUse" val="true"/>
        <set name="hitCancelTime" val="500"/>
        <for>
            <effect name="Buff" time="30" val="0" stackType="pAtk" stackOrder="5">
                <mul stat="pAtk" order="0x30" val="2"/>
                <mul stat="mAtk" order="0x30" val="4"/>
            </effect>
        </for>
    </skill>

	<etcitem id="17526" name="Scroll: Enchant Weapon (R-grade)">
	<!-- Increases P. Atk. by 6 for one-handed R-grade weapons. Increases P. Atk. by 7 for two-handed R-grade weapons and Crossbows. Increases P. Atk. by 12 for Bows. Increases M. Atk. by 5 for all weapons. All amounts Increases starting at + 4. All amounts are further increased for blessed weapons. Enchanting is safe up to + 3. Can be exchanged or dropped. -->
		<set name="class" value="MISC"/>
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.scroll_of_enchant_weapon_i06"/>
		<set name="price" value="5000000"/>
		<set name="stackable" value="true"/>
		<set name="type" value="SCROLL"/>
		<set name="weight" value="120"/>
		<cond msgId="1508">
			<not>
				<player olympiad="true"/>
			</not>
		</cond>
	</etcitem>
	<etcitem id="17527" name="Scroll: Enchant Armor (R-grade)">
	<!-- When enchanted, increases P. Def. by 2 for R-grade armor and accessories. Greatly increased starting at + 4. Can be enhanced safely to + 3. When enhanced + 4 or more, HP also increases according to the enchant value. -->
		<set name="class" value="MISC"/>
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.scroll_of_enchant_armor_i06"/>

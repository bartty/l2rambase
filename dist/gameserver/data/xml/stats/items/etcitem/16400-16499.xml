	<etcitem id="16407" name="Santa's Stockings" add_name="Event">
	<!-- Santa's Stockings. By Double-clicking, you can obtain 1 bonus item with a certain chance. Cannot be exchanged, dropped, put in a private store, or destroyed. -->
		<set name="class" value="OTHER"/>
		<set name="ex_type" value="OTHER_ITEMS"/>
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.xmas_present_i00"/>
		<set name="stackable" value="true"/>
		<set name="type" value="OTHER"/>
		<cond msgId="1508">
			<not>
				<player olympiad="true"/>
			</not>
		</cond>
		<skills>
			<skill id="9080" level="1"/>
		</skills>
		<set name="capsuled" value="true"/>
	</etcitem>

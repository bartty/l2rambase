	<etcitem id="15711" name="War Veteran Voucher">
	<!-- Document showing military service in the Seed of Annihilation region. Collect plenty of these and take them back to Soldier Ilvenis at the pier in the Seed of Annihilation to exchange for a reward. -->
		<set name="class" value="OTHER"/>
		<set name="ex_type" value="OTHER_ITEMS"/>
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.etc_charm_of_courage_i00"/>
		<set name="stackable" value="true"/>
		<set name="type" value="OTHER"/>
		<cond msgId="1508">
			<not>
				<player olympiad="true"/>
			</not>
		</cond>
	</etcitem>

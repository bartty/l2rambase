	<etcitem id="15716" name="Packaged Book">
	<!-- Item received after saving a Treasure Hunter. It's tightly wrapped so you can't see what book is inside. Double-click for more information. -->
		<set name="class" value="OTHER"/>
		<set name="ex_type" value="OTHER_ITEMS"/>
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.etc_memopad_i00"/>
		<set name="stackable" value="true"/>
		<set name="type" value="OTHER"/>

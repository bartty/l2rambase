	<etcitem id="20602" name="Soul Silver Foil (Event)">
	<!-- Silver Foil to comfort a spirit of monster.
	When used on a corpse of monster, increases max HP by 60%, max MP by 60%, max CP by 60%, P. Def. by 30%, Magic Def. by 30%, Speed by 30, P. Atk. by 20%, and decreases skill MP consumption by 10% for 2 min. Cannot be exchanged, dropped, or sold. Can be stored in a private warehouse. -->
		<set name="class" value="OTHER"/>
		<set name="ex_type" value="OTHER_ITEMS"/>
		<set name="crystal_type" value="NONE"/>
		<set name="dropable" value="false"/>
		<set name="icon" value="BranchSys.icon.br_ghost_silverpaper_i00"/>
		<set name="stackable" value="true"/>
		<set name="tradeable" value="false"/>
		<set name="type" value="OTHER"/>
		<cond msgId="1508">
			<not>
				<player olympiad="true"/>
			</not>
		</cond>
	</etcitem>
